<%= form_for(@notice) do |f| %>
<% if @notice.errors.any? %>
<%= render partial: "shared/errors", locals: { resource: @notice } %>
<% end %>

<div class="form-group">
                <%= f.label :actions %>
            <%= f.text_field :actions, :class => "form-control" %>
    </div>
<div class="form-group">
                <%= f.label :price %>
            <%= f.number_field :price, :class => "form-control" %>
    </div>
<div class="actions">
    <%= f.submit :class => "btn btn-success" %>
</div>
<% end %>
